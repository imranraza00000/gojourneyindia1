<%- include('./admindashboardsidebar.ejs')%>

<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Contact Enquiry</h1>


    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <!-- <th>S/n</th> -->
                            <th>Name</th>

                            <th>Mobile</th>
                            <th>Email</th>
                            <th>Messages</th>
                            <th>Date</th>


                        </tr>
                    </thead>

                    <tbody>


                        <% for(let i=0; i< tableData.length; i++){ %>
                        <tr>

                            <td>
                                <%= tableData[i].name %>
                            </td>

                            <td>
                                <%= tableData[i].mobile %>
                            </td>
                            <td>
                                <%= tableData[i].email %>
                            </td>



                            <td>
                                <%= tableData[i].message %>
                            </td>

                            <td>
                                <%= tableData[i].createdAt %>
                            </td>

                        </tr>
                        <% }%>

                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
<%- include('./admindashboardnavbar.ejs')%>
<!-- /.container-fluid -->